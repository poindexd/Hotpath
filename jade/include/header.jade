mixin links
	li.no: a(href='/surveys.html').orange-text Surveys
	li.no: a.orange-text About

header
	nav.white: .nav-wrapper(style='padding: 0 40px')
		a(href='/').brand-logo: img(src='./img/hotpath_logo.svg' height='100%' style='padding: 10px')
		a.button-collapse.orange-text(href='#' data-activates='side-nav')
			i.material-icons menu

		ul.right.hide-on-med-and-down
			+links
		ul#side-nav.side-nav
			+links